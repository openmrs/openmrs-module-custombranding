<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.openmrs.module.custombranding">
	    <class name="CssFile"
		   table="custombranding_css_file">
		<id name="cssFileId" type="int" column="css_file_id" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="css_file_id" insert="false" />
		<property name="name" type="java.lang.String" column="name" length="255" unique="true" />
		<property name="content" type="java.lang.String" column="content" length="65535"/>
		<property name="nameAndPath" type="java.lang.String" column="name_and_path" length="2048" unique="true"/>
		<property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
	</class>
</hibernate-mapping>